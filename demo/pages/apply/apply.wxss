.titleBar {
  width: 100%;
  height: 150rpx;
  background-color: #0e7cff;
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  align-items: center;
}

.titleBar .title {
  margin-top: 30rpx;
  font-size: 28rpx
}

.goLeft {
  margin-top: 15rpx;
  margin-left:15rpx;
  width: 20rpx;
  height: 35rpx;
}

.turnBack {
  width: 100rpx;
  height: 34rpx;
  font-size: 36rpx;
  font-family: PingFang-SC-Bold;
  font-weight: bold;
  color: rgba(255, 255, 255, 1);
}

.title {
  width: 150rpx;
  height: 35rpx;
  font-size: 36rpx;
  font-family: Adobe Heiti Std R;
  font-weight: normal;
  color: rgba(255, 255, 255, 1);
  margin-left: 180rpx;
}

.showTable {
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
  align-items: center;
}

.tableBox {
  width: 90%;
  display: flex;
  flex-direction: column;
  justify-content: space-around;
  align-items: flex-start;
  margin-top: 50rpx;
  background: rgba(255, 255, 255, 1);
  box-shadow: 0px 1px 15px 3px rgba(14, 124, 255, 0.08);
  border-radius: 6px;
}

.rowTitle {
  width: 100%;
  font-size: 26rpx;
  font-family: PingFang-SC-Bold;
  font-weight: bold;
  color: rgba(51, 51, 51, 1);
  display: flex;
  flex-direction: row;
  justify-content: space-around;
  align-items: center;
}

.massges {
  width: 100%;
  display: flex;
  flex-direction: row;
  justify-content: space-around;
  align-items: center;
}

.column_1 {
  flex: 4;
  font-size: 24rpx;
  font-family: PingFang-SC-Bold;
  font-weight: bold;
  color: rgba(102, 102, 102, 1);
  text-align: center;
}

.column_2 {
  flex: 4;
  font-size: 24rpx;
  font-family: PingFang-SC-Bold;
  font-weight: bold;
  color: rgba(46, 137, 255, 1);
  text-align: center;
}

.column_3 {
  flex: 4;
  font-size: 24rpx;
  font-family: PingFang-SC-Bold;
  font-weight: bold;
  color: rgba(102, 102, 102, 1);
  text-align: center;
}

#pending {
  font-size: 24rpx;
  font-family: PingFang-SC-Bold;
  font-weight: bold;
  color: rgba(255, 46, 46, 1);
}

.over {
  font-size: 23rpx;
  font-family: PingFang-SC-Bold;
  font-weight: bold;
  color: rgba(199, 199, 199, 1);
  margin-top: 40rpx;
}

.column{
  margin-top: 10rpx;
}